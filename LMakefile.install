(:package lispmake)
(:toplevel main)
(:quicklisp (unix-options split-sequence))
(:lisp all)
(:build-with "/usr/local/bin/sbcl")
(:do-build)
(:file ("package.lisp" "lispmake.lisp"
	"lisp-specific.lisp" "plugin.lisp"
	"options.lisp"))
(:output "lispmake")
(:install (:file output :to "/usr/local/bin" :mode 555))

